@model ProductResult
@ViewData
<h1>Do you want to delete Name:<span class="alert-info">@Model.Name</span>, from Manufacturer: <span class="alert-info">@Model.Manufacturer</span> ?</h1>
<form method="post">
    <a asp-action="Index" class="btn btn-secondary">
         No
    </a>
    <button type="submit" class="btn btn-danger">
         Yes
    </button>
</form>
