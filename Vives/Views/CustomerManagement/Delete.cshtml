@model CustomerResult
<h1>Do you want to delete <span class="alert-info">@Model.FirstName</span>, <span class="alert-info">@Model.LastName</span> ?</h1>
<form method="post">
    <a asp-action="Index" class="btn btn-secondary">
         No
    </a>
    <button type="submit" class="btn btn-danger">
         Yes
    </button>
</form>
