@model ArticleResult
<h1>Do you want to delete <span class="alert-info">@Model.Id</span>, with status: <span class="alert-info">@Model.Status</span> ?</h1>
<form method="post">
    <a asp-action="Index" class="btn btn-secondary">
         No
    </a>
    <button type="submit" class="btn btn-danger">
         Yes
    </button>
</form>
