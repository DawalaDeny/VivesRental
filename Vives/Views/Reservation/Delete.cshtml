@model ArticleReservationResult
@{
    var productName = ViewBag.Name;
    var customerName = ViewBag.Customer;
}
<h1>Do you want to delete product: <span class="alert-info">@productName</span>, for customer: <span class="alert-info">@customerName</span> ?</h1>
<form method="post">
    <a asp-action="Index" class="btn btn-secondary">
         No
    </a>
    <button type="submit" class="btn btn-danger">
         Yes
    </button>
</form>
